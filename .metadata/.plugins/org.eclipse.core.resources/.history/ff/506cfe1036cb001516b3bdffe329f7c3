package com.controller;

import java.util.UUID;

public class Home {
	public static void main( String[] args )
    {
        Home obj = new Home();
        System.out.println("Unique ID : " + obj.generateUniqueKey());
    }
    
    public String generateUniqueKey(){
    	
    	String id = UUID.randomUUID().toString();
    	return id;
    	
    }
}
